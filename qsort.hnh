
(++) l1 l2 = 
     let
	f [] = l2;
	f (x:xs) = (Cons x (f xs)); 
     in
	f l1; 

filter f l =
  let
    fil l:ls = (if f l then Cons l (fil ls) else fil ls); 
    fil [] = [];
  in
    fil l;

not True = False;
not False = True;

(<=) a b = not (a > b);


qsort []     = [];
qsort (x:xs) = (qsort (filter ((>) x) xs)) ++ [x] ++ (qsort (filter ((<=) x) xs));




ord = qsort [1,3,2,1,9,8,7,6,5,4,3,4,5,6,7,8,9,10,0];
