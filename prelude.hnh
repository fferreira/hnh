
infixl 6 +, -, +., -.
infixl 7 *, /, *., /. 
--infixr 8 ^,^.
infix 4 ==, <, >

infixr 5 ++,<> -- <> should be :

-- infixr 3 &&
-- infixr 2 ||

(<>) a b = Cons a b ;

(++) (x:xs) l2 = x <> (xs ++ l2); 
(++) [] l2 = l2;

filter f l =
  let
    fil l:ls = (if f l then l<>(fil ls) else fil ls); 
    fil [] = [];
  in
    fil l;

not True = False;
not False = True;

(<=) a b = not (a > b);
